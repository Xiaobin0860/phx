<h2>Show User</h2>

<ul>

  <li>
    <strong>Account:</strong>
    <%= @user.account %>
  </li>

  <li>
    <strong>Pass:</strong>
    <%= @user.pass %>
  </li>

  <li>
    <strong>Nick:</strong>
    <%= @user.nick %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: user_path(@conn, :index) %></span>
